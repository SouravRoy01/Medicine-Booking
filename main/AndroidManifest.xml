<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.souravroy.med4u">

    <application
        android:allowBackup="true"
        android:icon="@drawable/me1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AllSearch" />
        <activity android:name=".Medicine" />
        <activity android:name=".Cart"></activity>
        <activity android:name=".Address"></activity>
        <activity android:name=".Order_Successful"></activity>
        <activity android:name=".Order_Summary"></activity>
        <activity android:name=".Payment_Details"></activity>
        <uses-feature
            android:name="android.hardware.camera"
            android:required="true" />

        <activity
            android:name=".Fax"
            android:label="@string/title_activity_fax"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AboutUs"
            android:label="@string/title_activity_about_us"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TermsAndConditions"
            android:label="@string/title_activity_terms_and_conditions"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RateUs"
            android:label="@string/title_activity_rate_us"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>